<div class="public-articleList lh-lotterySkill2">
    <div class="public-titleBlock">
        <i class="icon-zx"></i>
        <span class="name">技巧</span>
        {{if type == '11x5'}}
        <a href="/article/list-11x5-32-p1" class="more" rel="nofollow" target='_blank'>更多>></a>
        {{else if type == 'k3'}}
        <a href="/article/list-k3-34-p1" class="more" rel="nofollow" target='_blank'>更多>></a>
        {{else if type == 'ssc'}}
        <a href="/article/list-ssc-35-p1" class="more" rel="nofollow" target='_blank'>更多>></a>
        {{else if type == 'kl10'}}
        <a href="/article/list-kl10-33-p1" class="more" rel="nofollow" target='_blank'>更多>></a>
        {{/if}}
    </div>
    <div class="al_listBlock">
        <ul>
            {{each tecInfo tec}}
            {{if type == '11x5'}}
            <li><a href="/article/detail-32-{{tec.id}}.html" class="title" target='_blank'>{{tec.title}}</a></li>
            {{else if type == 'k3'}}
            <li><a href="/article/detail-34-{{tec.id}}.html" class="title" target='_blank'>{{tec.title}}</a></li>
            {{else if type == 'ssc'}}
            <li><a href="/article/detail-35-{{tec.id}}.html" class="title" target='_blank'>{{tec.title}}</a></li>
            {{else if type == 'kl10'}}
            <li><a href="/article/detail-33-{{tec.id}}.html" class="title" target='_blank'>{{tec.title}}</a></li>
            {{/if}}
            {{/each}}
            
        </ul>
    </div>
</div>
