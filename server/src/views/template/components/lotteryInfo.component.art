        <ul>
            {{each pointInfo.items point}}
            <li>
                <a href="/point/detail-{{point.id}}.html" style="width: 220px" class="title" target='_blank'>{{point.title}}</a></span>
            </li>
            {{/each}}
        </ul>
    </div>
</div>
