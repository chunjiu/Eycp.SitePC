<!-- 高频彩票 if item.groupChildId !== 5 -->
{{if list.high}}
<div class="cp-trend">
    <div class="bigTitle lh-titleB" data-type="2" {{if groupType != 0 && groupType != 2}}style="display:none;"{{/if}}>高频彩票</div>
    <ul class="trend-ul public-table" data-type="2" {{if groupType != 0 && groupType != 2}}style="display:none;"{{/if}}>
        {{each list.high item idx}}
        <li class="trend-li" data-highCategory="{{item.groupChildId}}">
            <div class="trend-left">
                <a target="_blank" href="/high/{{item.lotteryCode}}">
                    <img alt="{{item.lotteryName}}" src="{{cdnUrl}}/pic/logo/logo_{{item.lotteryCode}}.png">
                </a>
            </div>
            <div class="trend-right">
                <p>{{item.lotteryName}}走势图表</p>
                <ul class="trend-r-content">
                   {{each highTrendData(item.groupChildId,item.lotteryCode).child highTrendItem idx}}
                    <li>
                        <span>{{highTrendItem.name}} :</span>
                        <div class="trend-choice">
                           {{each highTrendItem.child highTrendChild i}}
                            <a href="/chart/{{item.lotteryCode}}/{{highTrendChild.code}}" target="_blank">{{highTrendChild.name}}</a>
                            {{if i !== highTrendItem.child.length-1}}
                            <i class="gray-line">|</i>
                            {{/if}}
                            {{/each}}
                        </div>
                    </li>
                    {{/each}}
                </ul>
            </div>
        </li>
        {{/each}}
    </ul>
</div>
{{/if}}

